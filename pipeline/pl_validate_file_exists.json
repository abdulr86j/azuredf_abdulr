{
	"name": "pl_validate_file_exists",
	"properties": {
		"activities": [
			{
				"name": "get_file_metadata_config",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "select \r\nmax(case when property_name = 'container_name' then property_value end) container_name,\r\nmax(case when property_name = 'folder_name' then property_value end) folder_name,\r\nmax(case when property_name = 'file_name' then property_value end) file_name\r\nfrom  dbo.etl_metadata_config",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_sqldb_sql",
						"type": "DatasetReference"
					},
					"firstRowOnly": true
				}
			}
		],
		"parameters": {
			"p_etl_job_name": {
				"type": "string"
			}
		},
		"folder": {
			"name": "DEisKool/ETL_pipline"
		},
		"annotations": []
	}
}