			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'localSqlServer'"
		},
		"ls_ablob_azuredatafactoryabdulr_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_azuredatafactoryabdulr'"
		},
		"ls_dl_abdatafactorydl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dl_abdatafactorydl'"
		},
		"ls_dl_orders_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dl_orders'"
